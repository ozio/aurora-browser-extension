{
  "name": "aurora-chrome-extension",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "scripts": {
    "build": "npm run build-ext; npm run build-sdk",
    "build-ext": "rm -rf ./extensions/build; webpack --config ./extensions/webpack.config.js",
    "dev-ext": "rm -rf ./extensions/build; webpack --config ./extensions/webpack.config.js --watch",
    "build-sdk": "rm -rf ./sdk/build; webpack --config ./sdk/webpack.config.js",
    "dev-sdk": "rm -rf ./sdk/build; webpack --config ./sdk/webpack.config.js --watch",
    "server": "node ./server/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bowser": "^1.9.2",
    "cli-highlight": "^1.2.3",
    "eventemitter3": "^3.0.1",
    "moment": "^2.20.1",
    "reconnecting-websocket": "^3.2.2"
  },
  "devDependencies": {
    "@types/bowser": "^1.1.1",
    "@types/chrome": "0.0.59",
    "@types/firefox-webext-browser": "^58.0.1",
    "awesome-typescript-loader": "^3.4.1",
    "file-loader": "^1.1.6",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.1",
    "webpack": "^3.11.0",
    "ws": "^4.0.0"
  }
